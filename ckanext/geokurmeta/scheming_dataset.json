{
  "scheming_version": 1,
  "dataset_type": "dataset",
  "about": "CKAN dataset schema for GeoKur",
  "about_url": "http://github.com/ckan/ckanext-scheming",
  "dataset_fields": [
    {
      "field_name": "title",
      "label": "Title",
      "preset": "title",
      "form_placeholder": "eg. A descriptive title"
    },
    {
      "field_name": "name",
      "label": "Identifier",
      "preset": "dataset_slug",
      "form_placeholder": "eg. my-dataset"
    },
    {
      "field_name": "owner_org",
      "label": "Organization",
      "preset": "dataset_organization"
    },
    {
      "field_name": "notes",
      "label": "Description",
      "validators": "not_empty",
      "form_snippet": "markdown.html",
      "form_placeholder": "eg. Some useful notes about the data"
    },
    {
      "field_name": "name",
      "label": "Contact Point: Name",
      "form_placeholder": "Person or Organization, e.g. EEA"
    },
    {
      "field_name": "url",
      "label": "Contact Point: URL",
      "form_placeholder": "according URL, e.g. http://www.eea.europa.eu"
    },
    {
      "field_name": "tag_string",
      "label": "Keywords",
      "preset": "tag_string_autocomplete_fix",
      "form_placeholder": "eg. economy, mental health, government"
    },
    {
      "field_name": "spatial",
      "label": "Spatial Coverage",
      "preset": "geokur_spatial",
      "display_snippet": "json.html"
    },
    {
      "field_name": "temporal_start",
      "label": "Temporal Coverage Start",
      "validators": "ignore_missing",
      "form_snippet": "date.html",
      "form_placeholder": ""
    },
    {
      "field_name": "temporal_end",
      "label": "Temporal Coverage End",
      "validators": "ignore_missing",
      "form_snippet": "date.html",
      "form_placeholder": ""
    },
    {
      "field_name": "theme",
      "label": "Theme / Category",
      "help_text": "Links to INSPIRE ontology: https://inspire.ec.europa.eu/theme . To define multiple links, separate them with a comma, e.g 'http://inspire.ec.europa.eu/theme/lc, http://inspire.ec.europa.eu/theme/rs'",
      "preset": "link_list_man"
    },
    {
      "field_name": "documentation",
      "label": "Documentation",
      "help_text": "eg. Links to publication",
      "preset": "link_single_man"
    },
    {
      "field_name": "has_version",
      "label": "Has Version",
      "help_text": "Identifier of related datasets, e.g. surface-water-bodies",
      "preset": "dataset_list"
    },
    {
      "field_name": "is_version_of",
      "label": "Is Version of",
      "help_text": "Identifier of related dataset, e.g. surface-water-bodies",
      "preset": "dataset_single"
    },
    {
      "field_name": "url",
      "label": "Landing Page",
      "help_text": "links to project website or dataset's page",
      "preset": "link_single"
    },
    {
      "field_name": "alternate_identifier",
      "label": "DOI Link",
      "help_text": "e.g. https://doi.org/10.7283/T5MW2F2D",
      "preset": "link_single"
    },
    {
      "field_name": "geokur_parent_dataset",
      "label": "Parent Dataset",
      "help_text": "Identifier of parent dataset, e.g. surface-water-bodies",
      "preset": "dataset_single"
    },
    {
      "field_name": "conforms_to",
      "label": "Reference System",
      "help_text": "Links to OGC EPSG Register, e.g. http://www.opengis.net/def/crs/EPSG/0/4326",
      "preset": "link_single"
    },
    {
      "field_name": "spatial_resolution_in_meters",
      "label": "Spatial Resolution in Meters",
      "validators": "ignore_missing unicode decimal_validator",
      "help_text": "Minimum spatial separation resolvable in a dataset, measured in meters."
    },
    {
      "field_name": "spatial_resolution_comment",
      "label": "Spatial Resolution as Equivalent Scale, Angular Distance, Vertical Distance",
      "validators": "ignore_missing unicode"
    },
    {
      "field_name": "temporal_resolution",
      "label": "Temporal Resolution",
      "validators": "ignore_missing unicode",
      "form_placeholder": "",
      "help_text": "e.g. 'P2Y3M20D' descibes a period 2 years, 3 months and 20 days or 'P1D' descibes a period of 1 day"
    },
    {
      "field_name": "prov_generated_by",
      "label": "Generated by",
      "help_text": "e.g. http://www.w3.org/ns/prov#",
      "preset": "link_single"
    },
    {
      "field_name": "prov_used_by",
      "label": "Used by",
      "help_text": "e.g. http://www.w3.org/ns/prov#",
      "preset": "link_list"
    },
    {
      "field_name": "has_quality_annotation",
      "label": "Has Quality Annotation",
      "validators": "ignore_missing unicode",
      "form_placeholder": ""
    }
  ],
  "resource_fields": [
    {
      "field_name": "url",
      "label": "URL",
      "preset": "resource_url_upload"
    },
    {
      "field_name": "name",
      "label": "Name",
      "validators": "if_empty_same_as(url) unicode",
      "form_placeholder": "eg. January 2011 Gold Prices"
    },
    {
      "field_name": "description",
      "label": "Description",
      "form_snippet": "markdown.html",
      "form_placeholder": "Some useful notes about the data"
    },
    {
      "field_name": "format",
      "label": "Format",
      "preset": "resource_format_autocomplete_fix"
    }
  ]
}